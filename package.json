{
  "name": "morib-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "src/main.cjs",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "electron": "NODE_ENV=development electron .",
    "electron:dev": "concurrently \"pnpm dev\" \"pnpm electron\"",
    "dist": "electron-builder",
    "dist:win": "electron-builder --win",
    "build:mac": "NODE_ENV=production vite build && electron-builder --mac --config electron-builder-config.cjs",
    "build:win": "NODE_ENV=production vite build && electron-builder --win --x64 --config electron-builder-config.cjs"
  },
  "dependencies": {
    "@remix-run/router": "^1.17.0",
    "@tanstack/react-query": "^5.49.2",
    "@types/lodash": "^4.17.7",
    "axios": "^1.7.2",
    "dayjs": "^1.11.11",
    "jotai": "^2.8.4",
    "react": "^18.3.1",
    "react-datepicker": "^7.3.0",
    "react-dom": "^18.3.1",
    "react-lottie": "^1.2.4",
    "react-router-dom": "^6.24.0",
    "socket.io-client": "^4.7.5",
    "timezone": "^1.0.23"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.49.1",
    "@tanstack/react-query-devtools": "^5.66.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/react": "^18.3.3",
    "@types/react-datepicker": "^6.2.0",
    "@types/react-dom": "^18.3.0",
    "@types/react-lottie": "^1.2.10",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^9.1.2",
    "electron": "^34.2.0",
    "electron-builder": "^25.1.8",
    "electron-notarize": "^1.2.2",
    "electron-vite": "^3.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "msw": "^2.7.0",
    "postcss": "^8.4.39",
    "prettier": "^3.3.2",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.2",
    "vite": "^5.3.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
